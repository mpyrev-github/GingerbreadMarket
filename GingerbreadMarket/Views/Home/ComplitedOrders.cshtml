@model IEnumerable<Deal>

<div class="panel-body">
    <table class="table table-striped table-condensed">
        <thead>
            <tr>
                <th>Выполнено</th>
                <th>Дата К</th>
                <th>Дата П</th>
                <th>Цена, $</th>
                <th>Количество</th>
                <th>Email К</th>
                <th>Email П</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.DealDate</td>
                    <td>@item.BuyDate</td>
                    <td>@item.SellDate</td>
                    <td>@item.Price</td>
                    <td>@item.Count</td>
                    <td>@item.BuyEmail</td>
                    <td>@item.SellEmail</td>
                </tr>
            }
        </tbody>
    </table>
</div>