@model IEnumerable<Order>

<div class="panel-body">
    <table class="table table-striped table-condensed">
        <thead>
            <tr><th>Цена, $</th><th>Количество</th><th>Email</th></tr>
        </thead>
        <tbody>
            @foreach (var item in Model.OrderBy(obj => obj.Price))
            {
                @if (item.IsSell)
                {
                    <tr>
                        <td>@item.Price</td>
                        <td>@item.Count</td>
                        <td>@item.Email</td>
                    </tr>
                }
            }
        </tbody>
    </table>
</div>